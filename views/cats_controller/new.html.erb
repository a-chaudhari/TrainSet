<form action="/cats" method="POST">
  <label>Name
    <input type="text" name="cat[name]" value="">
  </label><br>
  <div class="">  <%= flash[:errors][:name] unless flash[:errors].nil? %></div>
  <label>Owner
  <select class="" name="cat[owner_id]">
    <option value="NULL">No one</option>
    <% Human.all.each do |human| %>
    <option value="<%= human.id %>"><%= human.fname + " " + human.lname %></option>
    <% end %>
  </select>
  </label><br>

  <input type="submit" value="Create Kitty">
</form>
